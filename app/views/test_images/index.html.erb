<h1>Listing test_images</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_images.each do |test_image| %>
      <tr>
        <td><%= test_image.name %></td>
        <td><%= test_image.description %></td>
        <td><%= test_image.image %></td>
        <td><%= link_to 'Show', test_image %></td>
        <td><%= link_to 'Edit', edit_test_image_path(test_image) %></td>
        <td><%= link_to 'Destroy', test_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test image', new_test_image_path %>
